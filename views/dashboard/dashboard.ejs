<script>
  <%-"var labels="+JSON.stringify(labels) %>
</script>
<% var url="?filter" %>
<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">Dashboard</h1>
  </div>
  <!-- /.col-lg-12 -->
</div>
<div class="row">
  <div class="col-md-4">
    <%- partial("panel" , {label : "Temperature" , icon : "fa-thermometer-full" ,measure : {sensor : "temp" , data : data.temp , color : "rgba(255 , 0 , 0 , 0.5)" }}) %>
  </div>
  <div class="col-md-4">
    <%- partial("panel" , {label : "Brightness" , icon : "fa-sun-o", measure : {sensor : "brightness" , data : data.brightness , color : "rgba( 0, 0 , 255 , 0.5)" }}) %>
  </div>
  <div class="col-md-4">
    <%- partial("panel" , {label : "Soil moisture" ,icon : "fa-tint" , measure : {sensor : "soil_moisture" , data : data.soil_moisture , color : "rgba(105, 178, 65 , 0.5)" }}) %>
  </div>
  <div class="col-md-4">
    <%- partial("panel" , {label : "Humidity" ,icon : "fa-yelp" ,measure : {sensor : "humidity" , data : data.humidity , color : "rgba( 0, 0 , 0 , 0.5)" }}) %>
  </div>
  <div class="col-md-4">
    <%- partial("panel" , {label : "Gas" , icon : "fa-exclamation-circle", measure : {sensor : "gas" , data : data.gas , color : "rgba( 255, 0 , 255 , 0.5)" }}) %>
  </div>
  <div class="col-md-4">
    <%- partial("panel" , {label : "Barometric pressure" , icon : "fa-tachometer", measure : {sensor : "pressure" , data : data.pressure , color : "rgba( 244, 244 , 0 , 0.5)" }}) %>
  </div>



</div>
